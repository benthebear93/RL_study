<?xml version="1.0"?>
<launch>
  <!-- load the joint state controller -->
  <rosparam file="$(find onelink_robot)/config/joint_state_controller.yaml" command="load" />
  <node name="joint_state_controller_spawner" pkg="controller_manager" type="spawner" args="joint_state_controller" />

  <!-- load the arm controller -->
  <rosparam file="$(find onelink_robot)/config/onelink_robot_arm_controller.yaml" command="load" />
  <node name="onelink_robot_controller_spawner" pkg="controller_manager" type="spawner" args="arm_controller" />
</launch>
